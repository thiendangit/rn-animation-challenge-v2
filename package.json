{
  "name": "expo-and-typescript",
  "version": "0.0.0",
  "description": "Showcase of an Expo app coded in TypeScript.",
  "author": {
    "email": "jan@aagaard.net",
    "name": "Jan Aagaard"
  },
  "private": true,
  "license": "MIT",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "yarn run start --android",
    "check-types": "tsc",
    "ios": "yarn run start --ios",
    "lint": "eslint --format unix \"**/*.ts\" \"**/*.tsx\"",
    "start": "yarn install && expo start",
    "validate": "yarn install --frozen-lockfile && yarn run lint && yarn run check-types"
  },
  "engines": {
    "node": ">=10",
    "yarn": ">=1.1"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.5",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "^5.9.8",
    "@react-navigation/stack": "^5.14.9",
    "expo": "^42.0.3",
    "expo-analytics-amplitude": "~10.2.0",
    "expo-apple-authentication": "~3.2.1",
    "expo-asset": "~8.3.3",
    "expo-av": "~9.2.3",
    "expo-barcode-scanner": "~10.2.2",
    "expo-blur": "~9.0.3",
    "expo-camera": "~11.2.2",
    "expo-constants": "~11.0.2",
    "expo-facebook": "~11.3.1",
    "expo-font": "~9.2.1",
    "expo-haptics": "~10.1.0",
    "expo-linear-gradient": "~9.2.0",
    "expo-local-authentication": "~11.1.1",
    "expo-sensors": "~10.2.2",
    "react": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-gesture-handler": "~1.10.3",
    "react-native-maps": "0.28.0",
    "react-native-reanimated": "~2.2.2",
    "react-native-redash": "^16.2.2",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-svg": "12.1.1",
    "react-native-unimodules": "^0.14.8"
  },
  "devDependencies": {
    "@babel/core": "~7.9.6",
    "@types/react": "~16.9.56",
    "@types/react-native": "~0.63.53",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-preset-expo": "8.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "expo-cli": "^4.11.0",
    "prettier": "^2.4.1",
    "typescript": "~4.0.8"
  }
}
